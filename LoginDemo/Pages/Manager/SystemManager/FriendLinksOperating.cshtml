@page "{id:int?}"
@model LoginDemo.Pages.Manager.SystemManager.FriendLinksOperatingModel
@{
    ViewData["Title"] = "友情链接操作";
}

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="favicon.ico">
    <link href="~/dist/css/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet">
    <link href="~/dist/css/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet">
    <link href="~/dist/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="~/dist/css/animate.min.css" rel="stylesheet">
    <link href="~/dist/css/style.min862f.css?v=4.1.0" rel="stylesheet">
    <script src="~/js/jquery.min.js?v=2.1.4"></script>
    <script src="~/js/bootstrap.min.js?v=3.3.6"></script>
    <script src="~/js/plugins/peity/jquery.peity.min.js"></script>
    <script src="~/js/content.min.js?v=1.0.0"></script>
    <script src="~/js/plugins/iCheck/icheck.min.js"></script>
    <script src="~/js/demo/peity-demo.min.js"></script>
    <script src="~/js/CZBJavaScript.js"></script>
    <script src="~/ueditor/ueditor.config.js"></script>
    <script src="~/ueditor/ueditor.all.js"></script>
    <script src="~/ueditor/lang/zh-cn/zh-cn.js"></script>
    <script src="~/js/jquery.min.js"></script>
</head>

<body class="gray-bg">
    <div class="wrapper wrapper-content animated fadeInRight">

        <div class="row">
            <div class="col-sm-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>友情链接操作</h5>
                    </div>
                    <div class="ibox-content">
                        <form method="post" class="form-horizontal">
                            <input asp-for="GetT.Id" hidden />
                            <div class="form-group">
                                <label class="col-sm-2 control-label">友情链接名称</label>
                                <div class="col-sm-10">
                                    <input asp-for="GetT.Linkname" class="form-control" />
                                </div>
                            </div>
                            <div class="hr-line-dashed"></div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">友情链接地址</label>
                                <div class="col-sm-10">
                                    <input asp-for="GetT.Url" class="form-control" />
                                </div>
                            </div>
                            <div class="hr-line-dashed"></div>
                            <div class="form-group">
                                <div class="col-sm-4 col-sm-offset-2">
                                    <button class="btn btn-primary" type="submit">保存内容</button>
                                    <button class="btn btn-white" type="button" onclick="history.back()">取消</button>
                                </div>
                            </div>

                        </form>
                    </div>

                </div>
            </div>


        </div>
    </div>

</body>



